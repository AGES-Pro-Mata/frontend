{
  "name": "pro-mata-frontend",
  "version": "1.0.0",
  "description": "React frontend for Pro-Mata reservation platform",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "preview": "vite preview --host 0.0.0.0",
    "serve": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:css": "stylelint \"src/**/*.{css,scss,sass}\"",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss,sass}\" --fix",
    "test": "vitest",
    "test:unit": "vitest run src/**/*.test.*",
    "test:component": "vitest run src/components/**/*.test.*",
    "test:integration": "vitest run src/tests/integration/**/*.test.*",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=junit --outputFile=test-results/junit.xml",
    "security-check": "npm audit --audit-level=high",
    "security-fix": "npm audit fix",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "clean": "rm -rf dist coverage test-results .cache",
    "clean:deps": "rm -rf node_modules package-lock.json",
    "clean:all": "npm run clean && npm run clean:deps",
    "docker:build": "docker build -t pro-mata-frontend:local -f Dockerfile.prod .",
    "docker:build:dev": "docker build -t pro-mata-frontend:dev -f Dockerfile.dev .",
    "docker:run": "docker run -p 3000:3000 --name pro-mata-frontend pro-mata-frontend:local",
    "docker:stop": "docker stop pro-mata-frontend && docker rm pro-mata-frontend",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "compose:logs": "docker-compose logs -f",
    "compose:build": "docker-compose build",
    "generate:component": "plop component",
    "generate:page": "plop page",
    "generate:hook": "plop hook",
    "lighthouse": "lhci autorun",
    "lighthouse:mobile": "lhci autorun --config=lighthouse.mobile.config.js",
    "deps:check": "npm-check-updates",
    "deps:update": "npm-check-updates -u && npm install",
    "deps:outdated": "npm outdated",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "start": "npm run dev",
    "start:prod": "npm run build && npm run preview",
    "start:docker": "npm run docker:build && npm run docker:run"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@tanstack/react-query": "^5.84.1",
    "@tanstack/react-query-devtools": "^5.84.1",
    "@tanstack/react-router": "^1.130.12",
    "@tanstack/router-devtools": "^1.130.13",
    "axios": "^1.6.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.300.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-hot-toast": "^2.4.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.12.0",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/router-vite-plugin": "^1.130.15",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/node": "^20.19.9",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-v8": "^1.0.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^7.1.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "husky": "^8.0.0",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.0.0",
    "npm-check-updates": "^16.0.0",
    "playwright": "^1.40.0",
    "plop": "^4.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.2.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,sass}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
